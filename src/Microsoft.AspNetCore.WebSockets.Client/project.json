{
  "version": "0.1.0-*",
  "description": "Client for communicating over web sockets.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/websockets"
    }
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.WebSockets.Protocol": "0.1.0-rc2-final"
  },
  "frameworks": {
    "net451": {}
  }
}